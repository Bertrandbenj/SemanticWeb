PREFIX  ex: <http://ex.org/a#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX sch: <http://schema.org/>
PREFIX xml: <http://www.w3.org/XML/1998/namespace>
PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX : <http://www.data.gov.ma/Education#>
SELECT (str(?name)AS ?name2) (str(?address) AS ?address2) ?city ?region ?status
WHERE { 	   
   		?x sch:name ?name;
	 	     sch:streetAddress ?address;
	                 sch:addressRegion ?region;
	  	     sch:addressLocality ?city;
                    	     :status ?status.
  FILTER( regex(str(?region),"Sous-Massa-Draa", "i")  &&   lang(?name)="fr-FR" && lang(?address)="fr-FR" && regex(str(?status),"prive", "i") )
}
  ORDER BY ?name
